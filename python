FROM python3.6:latest

LABEL AUTHOR="Scott Businge <businge.scott@andela.com>"
LABEL app="app-name"

WORKDIR /app

COPY folder/file folder/file
COPY folder/file folder/file

COPY src /app

COPY scripts/migration.sh scripts/migration.sh
RUN chmod +x scripts/migration.sh

RUN chmod +x start.sh
CMD [ "start.sh" ]